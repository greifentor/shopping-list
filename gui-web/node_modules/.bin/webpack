#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ollie/eclipse-workspace-2021-09/shopping-list/gui-web/node_modules/.pnpm/webpack@4.46.0_webpack-cli@4.9.0/node_modules/webpack/bin/node_modules:/home/ollie/eclipse-workspace-2021-09/shopping-list/gui-web/node_modules/.pnpm/webpack@4.46.0_webpack-cli@4.9.0/node_modules/webpack/node_modules:/home/ollie/eclipse-workspace-2021-09/shopping-list/gui-web/node_modules/.pnpm/webpack@4.46.0_webpack-cli@4.9.0/node_modules:/home/ollie/eclipse-workspace-2021-09/shopping-list/gui-web/node_modules/.pnpm/node_modules:/home/ollie/eclipse-workspace-2021-09/shopping-list/gui-web/node_modules:/home/ollie/eclipse-workspace-2021-09/shopping-list/node_modules:/home/ollie/eclipse-workspace-2021-09/node_modules:/home/ollie/node_modules:/home/node_modules:/node_modules:/home/ollie/eclipse-workspace-2021-09/shopping-list/gui-web/node_modules/webpack/bin/node_modules:/home/ollie/eclipse-workspace-2021-09/shopping-list/gui-web/node_modules/webpack/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/ollie/eclipse-workspace-2021-09/shopping-list/gui-web/node_modules/.pnpm/webpack@4.46.0_webpack-cli@4.9.0/node_modules/webpack/bin/node_modules:/home/ollie/eclipse-workspace-2021-09/shopping-list/gui-web/node_modules/.pnpm/webpack@4.46.0_webpack-cli@4.9.0/node_modules/webpack/node_modules:/home/ollie/eclipse-workspace-2021-09/shopping-list/gui-web/node_modules/.pnpm/webpack@4.46.0_webpack-cli@4.9.0/node_modules:/home/ollie/eclipse-workspace-2021-09/shopping-list/gui-web/node_modules/.pnpm/node_modules:/home/ollie/eclipse-workspace-2021-09/shopping-list/gui-web/node_modules:/home/ollie/eclipse-workspace-2021-09/shopping-list/node_modules:/home/ollie/eclipse-workspace-2021-09/node_modules:/home/ollie/node_modules:/home/node_modules:/node_modules:/home/ollie/eclipse-workspace-2021-09/shopping-list/gui-web/node_modules/webpack/bin/node_modules:/home/ollie/eclipse-workspace-2021-09/shopping-list/gui-web/node_modules/webpack/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../webpack/bin/webpack.js" "$@"
else
  exec node  "$basedir/../webpack/bin/webpack.js" "$@"
fi
